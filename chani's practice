<h1>Array</h1>
<h2>Syntax</h2>
<script>
    var coworkers = ["egoing", "leexche"];
</script>
<h2>get</h2>
<script>
    document.write(coworkers[0]);
    document.write(coworkers[1]);
    coworkers.length;
</script>
<h2>add</h2>
<script>
    coworkers.push('duru');
    coworkers.push('taeho');
</script>
<h2>count</h2>
<script>
    document.write(coworkers.length);
</script>
